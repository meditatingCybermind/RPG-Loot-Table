{
  "name": "RPG_Loot-Table",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/meditatingCybermind/RPG-Loot-Table.git"
  },
  "dependencies": {
    "dexie": "^1.5.1",
    "express": "^4.14.1",
    "morgan": "^1.8.1",
    "nedb": "^1.8.0",
    "nodemon": "^1.9.2",
    "prompt": "^1.0.0",
    "stylus": "^0.54.5"
  },
  "devDependencies": {
    "electron": "^1.4.1",
    "karma-mocha": "^1.3.0",
    "mocha": "^3.2.0"
  },
  "scripts": {
    "build-css": "stylus ../client/index.styl -o ../client/static/css",
    "watch-css": "stylus ../client/index.styl -o ../client/static/css -w",
    "clean": "rm -rf ../client/static/css && mkdir -p ../client/static/css",
    "build": "npm run clean && npm run build-css",
    "watch": "npm run clean && npm run watch-css & nodemon --exec 'electron main.js' -e js,html --watch . --watch ../client",
    "start": "electron .",
    "test": "./node_modules/mocha/bin/mocha"
  },
